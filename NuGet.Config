<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<packageSources>
		<clear />
		<add key="github.com" value="https://nuget.pkg.github.com/TSSArt/index.json" />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="github.com">
			<package pattern="Xtate.*" />
		</packageSource>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
</configuration>